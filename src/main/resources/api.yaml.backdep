      title: IPage«BaseChainResponse»
      x-apifox-folder: Schemas
    ConfigParam:
      type: object
      properties:
        paramName:
          type: string
          description: 配置参数名称
        value:
          type: string
          description: 配置参数值
      title: ConfigParam
      x-apifox-folder: Schemas
    OrderExpireModel:
      type: object
      properties:
        autoRenew:
          type: boolean
          description: 是否自动续期
        duration:
          type: string
          description: 服务时长
        expireId:
          type: integer
          format: int64
          description: 订单过期ID
        expireTime:
          type: integer
          format: int64
          description: 到期时间
        orderId:
          type: integer
          format: int64
          description: 订单ID
        renewState:
          type: string
          description: 订单续期状态：UN_EXPIRED：未过期，EXPIRED:已过期, RENEWED:已续期
      title: OrderExpireModel
      description: OrderExpireModel
      x-apifox-folder: Schemas
