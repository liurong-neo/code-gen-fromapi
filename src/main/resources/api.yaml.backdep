openapi: 3.0.1
info:
  title: 链服务
  description: ''
  version: 1.0.0
tags:
  - name: chain-service
  - name: chain-service/chainOrder
  - name: chain-service/chain
  - name: chain-service/chain/chainVersion
  - name: chain-service/chain/consortium
  - name: chain-service/chain/chainParams
  - name: chain-service/chain/chainNode
  - name: chain-service/chain/para
  - name: chain-service/chainExplorer
  - name: chain-service/chainQuickQuery
paths:
  /v1/chainconfig/template/{type}:
    get:
      summary: 获取配置模板
      x-apifox-folder: chain-service/chain
      x-apifox-status: released
      deprecated: false
      description: 获取配置模板本
      tags:
        - 链配置
      parameters:
        - name: type
          in: path
          description: 共识类型 0联盟链，1私链,2平行链
          required: true
          example: ''
          schema:
            type: string
        - name: Object-Id
          in: header
          description: 对象ID
          required: false
          example: ''
          schema:
            type: string
        - name: Object-Type
          in: header
          description: 对象类型
          required: false
          example: ''
          schema:
            type: string
        - name: User-ID
          in: header
          description: 登录用户ID
          required: false
          example: ''
          schema:
            type: string
        - name: User-Name
          in: header
          description: 登录用户名称
          required: false
          example: ''
          schema:
            type: string
      responses:
        '200':
          description: OK
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/ConfigTemplate'
              examples: {}
        '401':
          description: Unauthorized
          content:
            application/json:
              schema:
                type: object
                properties: {}
              examples: {}
        '403':
          description: Forbidden
          content:
            application/json:
              schema:
                type: object
                properties: {}
              examples: {}
        '404':
          description: Not Found
          content:
            application/json:
              schema:
                type: object
                properties: {}
              examples: {}
  /v1/chainOrder/{orderId}:
    get:
      summary: 根据订单ID查看链订单
      x-apifox-folder: chain-service/chainOrder
      x-apifox-status: released
      deprecated: false
      description: ''
      tags:
        - 链订单
      parameters:
        - name: orderId
          in: path
          description: orderId
          required: true
          example: ''
          schema:
            type: string
      responses:
        '200':
          description: OK
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ChainOrderModel'
              examples: {}
        '401':
          description: Unauthorized
          content:
            application/json:
              schema:
                type: object
                properties: {}
              examples: {}
        '403':
          description: Forbidden
          content:
            application/json:
              schema:
                type: object
                properties: {}
              examples: {}
        '404':
          description: Not Found
          content:
            application/json:
              schema:
                type: object
                properties: {}
              examples: {}
      x-baas-ref-resources: chain
      x-baas-ref-apis: '{"$license-service-a$":["GET /v1/chain/manage/consortium/login-type","GET /v2/chain/consortium/page","DELETE /v2/chain/consortium/purge/{chain_id}"]}'
      x-baas-roles: manager,normal
components:
  schemas:
    BindPubIpRequest:
      type: object
      properties:
        driverId:
          type: object
          description: 云平台唯一ID
        ipName:
          type: string
          description: ip名称
        nodeId:
          type: string
          description: 节点Id
        payType:
          type: string
          example: prePaid, postPaid
          description: 支付类型
        prePaidParam:
          $ref: '#/components/schemas/PrePaidParam'
        profile:
          type: integer
          format: int64
          description: 关联的偏好信息
        publicIp:
          $ref: '#/components/schemas/PublicIp'
        serverId:
          type: string
          description: 云主机Id，可不传
      title: BindPubIpRequest
      x-apifox-folder: Schemas
    RecycleNodesRequest:
      type: object
      properties:
        chainId:
          type: string
          example: 全局唯一的UUID，前端填充
          description: 区块链ID
        nodes:
          type: array
          description: 节点列表
          items:
            type: string
      title: RecycleNodesRequest
      x-apifox-folder: Schemas
    IPage«BaseNodeResponse»:
      type: object
      properties:
        current:
          type: integer
          format: int64
        hitCount:
          type: boolean
        pages:
          type: integer
          format: int64
        records:
          type: array
          items:
            $ref: '#/components/schemas/BaseNodeResponse'
        searchCount:
          type: boolean
        size:
          type: integer
          format: int64
        total:
          type: integer
          format: int64
      title: IPage«BaseNodeResponse»
      x-apifox-folder: Schemas
    ChainNodeRequest:
      type: object
      properties:
        accessPassword:
          type: string
          description: 密码
        accessUser:
          type: string
          description: 登录用户名
        eid:
          type: string
          description: 企业id
        id:
          type: string
          description: 节点唯一id，前端使用UUID生成
        innerIp:
          type: string
          description: 内网ip
        name:
          type: string
          description: 节点名称
        orderId:
          type: string
          description: 订单编号
        outerIp:
          type: string
          description: 外网ip，当为云节点时：为空或和内网IP一致时表示不分配，为0.0.0.0时表示自动分配，其它值(暂未支持)表示已有地址
        sshKeyId:
          type: integer
          format: int32
          description: 密钥id
        sshPort:
          type: integer
          format: int32
          description: ssh端口
        sshPrivKey:
          type: string
          description: 私钥
        sshPubKey:
          type: string
          description: 公钥
        type:
          type: integer
          format: int32
          description: 节点类型
        usePrivKeyAccess:
          type: boolean
          description: 是否使用私钥登录
      title: ChainNodeRequest
      x-apifox-folder: Schemas
    Organization:
      type: object
      properties:
        address:
          type: string
          description: 企业地址
        addressList:
          type: array
          description: 地区
          items:
            type: string
        addressTran:
          type: string
          description: 地区
        certs:
          type: array
          description: 企业资质数据
          items:
            type: string
        createTime:
          type: integer
          format: int64
          description: 企业创建时间
        creator:
          type: string
          description: 企业创始人
        federations:
          type: array
          description: 企业加入的联盟
          items:
            type: string
        manage:
          type: string
          description: 经营范围
        orgID:
          type: string
          description: 企业id
        orgName:
          type: string
          description: 企业名
        owner:
          type: string
          description: 企业所有人
        ownerId:
          type: integer
          format: int64
          description: 企业所有人id
        remark:
          type: string
          description: 拒绝理由
        status:
          type: integer
          format: int32
          description: 企业状态
        type:
          type: string
          description: 所属行业
        userRoleList:
          type: array
          description: 用户在该企业的角色信息
          items:
            $ref: '#/components/schemas/UserRoleObj'
      title: Organization
      x-apifox-folder: Schemas
    UserRoleObj:
      type: object
      properties:
        roleId:
          type: string
          description: 角色id
        roleName:
          type: string
          description: 角色名称
      title: UserRoleObj
      x-apifox-folder: Schemas
    BaseNodeRequest:
      type: object
      properties:
        accessPassword:
          type: string
          description: 密码
        accessUser:
          type: string
          description: 登录用户名
        name:
          type: string
          description: 节点名称
        sshPort:
          type: integer
          format: int32
          description: ssh端口
      title: BaseNodeRequest
      x-apifox-folder: Schemas
    ChainExplorerNodeRequest:
      type: object
      properties:
        accessPassword:
          type: string
          description: 密码
        accessUser:
          type: string
          description: 登录用户名
        app_type:
          type: string
          description: 业务类型
        eid:
          type: string
          description: 企业id
        id:
          type: string
          description: 唯一id，前端使用UUID生成
        innerIp:
          type: string
          description: 内网ip
        name:
          type: string
          description: 节点名称
        orderId:
          type: string
          description: 订单编号
        outerIp:
          type: string
          description: 外网ip，当为云节点时：为空或和内网IP一致时表示不分配，为0.0.0.0时表示自动分配，其它值(暂未支持)表示已有地址
        sshPort:
          type: integer
          format: int32
          description: ssh端口
        sync_ip:
          type: string
          description: 服务ip
        sync_port:
          type: string
          description: 服务端口
      title: ChainExplorerNodeRequest
      x-apifox-folder: Schemas
    MainStatisticResponse:
      type: object
      properties:
        chainId:
          type: string
          description: 链ID
        chainVersion:
          type: string
          description: 链版本信息
        createTime:
          type: integer
          format: int64
          description: 创建时间
        createType:
          type: integer
          format: int32
          description: 创建类型
        creator:
          type: integer
          format: int64
          description: 创建者
        creatorName:
          type: string
          description: 创建者名字
        enterpriseCount:
          type: integer
          format: int32
          description: 企业成员数量
        fedName:
          type: string
          description: 联盟名称
        genesis:
          type: string
          description: 创世地址
        indicator:
          type: object
          description: 监控指标数据(map类型数据结构),activeNodes(存活的节点),totalTransaction(链上交易总数),maxTps(tps),avgBlockTime(平均出块时间),
        name:
          type: string
          description: 链名称
        nodeCount:
          type: integer
          format: int32
          description: 节点数量
        nodeList:
          type: array
          description: 节点列表
          items:
            $ref: '#/components/schemas/BaseNodeResponse'
        orgList:
          type: array
          description: 链节点对应的企业成员
          items:
            $ref: '#/components/schemas/Organization'
        owner:
          type: integer
          format: int64
          description: 拥有者
        ownerName:
          type: string
          description: 拥有者名字
        paraChainList:
          type: array
          description: 平行链列表
          items:
            $ref: '#/components/schemas/ParaChainResponse'
        paraCount:
          type: integer
          format: int32
          description: 挂载平行链条数
        status:
          type: integer
          format: int32
          description: 1 正常  0 异常
        superManger:
          type: string
          description: 超管地址名称
      title: MainStatisticResponse
      x-apifox-folder: Schemas
    QuickQueryModel:
      type: object
      properties:
        dataId:
          type: string
          description: 数据ID
        dataName:
          type: string
          description: 数据名称
        dataType:
          type: string
          description: 数据类型
          enum:
            - APPSTORE
            - CHAIN
            - CONSORTIUM
            - EMPTY
            - FED
            - ORDER
            - ORG
            - PARA
        detail:
          type: string
          description: 数据明细信息
        objPermission:
          type: array
          description: 数据查看所需要的权限
          items:
            type: string
        objectId:
          type: string
          description: 对象ID/控制台ID
        updateTime:
          type: integer
          format: int64
          description: 数据更新时间
      title: QuickQueryModel
      description: 快速检索对象
      x-apifox-folder: Schemas
    ChainOrderModel:
      type: object
      properties:
        address:
          type: string
          description: 管理员地址
        autoRenew:
          type: boolean
          description: 是否自动续费
        billingMode:
          type: string
          description: 计费模式
        chainId:
          type: string
          description: 联盟链/平行链ID，创建链时为空，添加节点时需要设置链ID
        chainType:
          type: string
          description: 链类型consortium/para
        coinSymbol:
          type: string
          description: 代币名称
        commodityInstanceId:
          type: string
          description: 商品实例ID:UUID
        createUserId:
          type: string
          description: 创建用户ID
        driver:
          type: string
          description: 云驱动key
        duration:
          type: string
          description: 时长
        genesis:
          type: string
          description: 创世地址
        genesisAmount:
          type: string
          description: 代币总数
        genesisBlockTime:
          type: string
          description: 创世区块时间
        grpcBindAddr:
          type: string
          description: grpc端口
        jrpcBindAddr:
          type: string
          description: jrpc端口
        mainChainId:
          type: string
          description: 主链ID,特指联盟链
        mainChainIp:
          type: string
          description: 主链IP,指定自定义主链
        maxTxNumber:
          type: string
          description: 最大打包交易数
        name:
          type: string
          description: 链名称
        nodeNum:
          type: integer
          format: int32
          description: 节点数量
        nodes:
          type: array
          description: 节点信息
          items:
            $ref: '#/components/schemas/ChainNodeRequest'
        orderId:
          type: integer
          format: int64
          description: 订单ID
        orderServiceType:
          type: string
          description: 订单服务：NEW：新增，ADD:增加(添加节点使用)
        p2pSeedPort:
          type: string
          description: seeds端口
        payerUser:
          type: string
          description: 支付用户
        profileId:
          type: string
          description: 套餐ID
        serviceType:
          type: string
          description: 服务类型CHAIN/CHAIN-CLOUD
        source:
          type: integer
          format: int32
          description: 来源类型0创建1导入
        startHeight:
          type: string
          description: 同步起始高度
        timeoutTxAvail:
          type: string
          description: 共识超时时间
        usePrivKeyAccess:
          type: boolean
          description: 登录类型false:账号密码登录true:私钥登录
        validatorNodePort:
          type: string
          description: validatorNode端口
      title: ChainOrderModel
      description: ChainOrderModel
      x-apifox-folder: Schemas
    ChainConfig:
      type: object
      properties:
        chainId:
          type: string
        createTime:
          type: integer
          format: int64
        id:
          type: integer
          format: int32
        isConfig:
          type: integer
          format: int32
        paramName:
          type: string
        updateTime:
          type: integer
          format: int64
        value:
          type: string
      title: ChainConfig
      x-apifox-folder: Schemas
    IPage«ChainSshKey»:
      type: object
      properties:
        current:
          type: integer
          format: int64
        hitCount:
          type: boolean
        pages:
          type: integer
          format: int64
        records:
          type: array
          items:
            $ref: '#/components/schemas/ChainSshKey'
        searchCount:
          type: boolean
        size:
          type: integer
          format: int64
        total:
          type: integer
          format: int64
      title: IPage«ChainSshKey»
      x-apifox-folder: Schemas
    OrderDetailModel:
      type: object
      properties:
        attributeLabel:
          type: string
          description: 订单属性展示值
        attributeMark:
          type: string
          description: 订单属性标志
        attributeName:
          type: string
          description: 订单属性名称
        attributeValue:
          type: object
          description: 订单属性值
        orderDetailId:
          type: integer
          format: int64
          description: 订单明细ID
        orderId:
          type: integer
          format: int64
          description: 订单ID
        parentId:
          type: integer
          format: int64
          description: 父ID
      title: OrderDetailModel
      description: 订单明细表
      x-apifox-folder: Schemas
    IPage«MainStatisticResponse»:
      type: object
      properties:
        current:
          type: integer
          format: int64
        hitCount:
          type: boolean
        pages:
          type: integer
          format: int64
        records:
