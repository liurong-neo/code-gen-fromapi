<component-set>
    <components>
        <component>
            <role>org.apache.maven.lifecycle.Lifecycle</role>
            <role-hint>api2fox</role-hint>
            <implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
            <configuration>
                <id>api2fox</id>
                <phases>
                    <phase>api2fox</phase>
                </phases>
                <default-phases>
                    <api2fox>com.fzm.maven.plugin:swagger-codegen-maven-plugin:api2fox</api2fox>
                </default-phases>
            </configuration>
        </component>
        <component>
            <role>org.apache.maven.lifecycle.Lifecycle</role>
            <role-hint>fox2api</role-hint>
            <implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
            <configuration>
                <id>fox2api</id>
                <phases>
                    <phase>fox2api</phase>
                </phases>
                <default-phases>
                    <fox2api>com.fzm.maven.plugin:swagger-codegen-maven-plugin:fox2api</fox2api>
                </default-phases>
            </configuration>
        </component>
        <component>
            <role>org.apache.maven.lifecycle.Lifecycle</role>
            <role-hint>codegen</role-hint>
            <implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
            <configuration>
                <id>codegen</id>
                <phases>
                    <phase>generate-sources</phase>
                </phases>
                <default-phases>
                    <generate-sources>com.fzm.maven.plugin:codegen-maven-plugin:generate-sources</generate-sources>
                </default-phases>
            </configuration>
        </component>
    </components>
</component-set>